<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:label="10Coffee"
        android:icon="@drawable/cup_coffee_cafe_drink_restaurant_hot_and_cold_icon_226129"
        android:roundIcon="@drawable/cup_coffee_cafe_drink_restaurant_hot_and_cold_icon_226129"
        android:supportsRtl="true"
        android:theme="@style/Theme._10MostConsumeCoffee"
        tools:targetApi="31">
        <activity
            android:label="@string/about_me"
            android:name=".AboutMe"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity" />

        <activity
            android:label="Detail Coffe"
            android:name=".DetailCoffe"
            android:exported="false"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>